<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="36dp"
            motion:layout_constraintHorizontal_bias="0.497"
            android:rotation="72" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="88dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintVertical_bias="1.0" />
        <Constraint
            android:layout_height="26dp"
            android:layout_width="27dp"
            android:id="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="@+id/imageView2"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView2"
            motion:layout_constraintTop_toTopOf="@+id/imageView2" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="88dp" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="30dp"
            android:id="@+id/imageView5"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="@+id/imageView4"
            motion:layout_constraintTop_toTopOf="@+id/imageView4" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="88dp" />
        <Constraint
            android:layout_height="25dp"
            android:layout_width="35dp"
            android:id="@+id/imageView8"
            motion:layout_constraintEnd_toEndOf="@+id/imageView6"
            motion:layout_constraintStart_toStartOf="@+id/imageView6"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView6"
            motion:layout_constraintTop_toTopOf="@+id/imageView6" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.675"
            motion:layout_constraintHorizontal_bias="0.22" />
        <Constraint
            android:layout_height="36dp"
            android:layout_width="26dp"
            android:id="@+id/imageView3"
            motion:layout_constraintTop_toTopOf="@+id/imageView2"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="@+id/imageView2" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="30dp"
            android:id="@+id/imageView5"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="@+id/imageView4" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="216dp"
            motion:layout_constraintHorizontal_bias="0.776" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView6"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="216dp" />
        <Constraint
            android:layout_height="25dp"
            android:layout_width="35dp"
            android:id="@+id/imageView8"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView6"
            motion:layout_constraintStart_toStartOf="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="@+id/imageView6"
            motion:layout_constraintTop_toTopOf="@+id/imageView6" />
    </ConstraintSet>
</MotionScene>